{
    "swagger": "2.0",
    "info": {
        "version": "0.0.1",
        "title": "file api"
    },
    "paths": {
        "/files/$id": {
            "get": {
                "description": "Gets `files` names.\nQuery param **$id** determines\nid of entreprise files\n",
                "parameters": [
                    {
                        "name": "id",
                        "in": "query",
                        "description": "id of entreprise",
                        "required": true,
                        "type": "number",
                        "format": "int"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "title": "tables_in_$id",
                            "type": "array",
                            "items": {
                                "title": "name",
                                "type": "object"
                            }
                        }
                    },
                    "404": {
                        "description": "entreprise not found"
                    }
                }
            }
        },
        "paths": {
        "/files/$id_entreprise/$id_fichier": {
            "get": {
                "description": "Gets `files` names.\nQuery param **$id** determines\nid of entreprise files\n",
                "parameters": [
                    {
                        "name": "id",
                        "in": "query",
                        "description": "id of entreprise",
                        "required": true,
                        "type": "number",
                        "format": "int"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "title": "tables_in_$id",
                            "type": "array",
                            "items": {
                                "title": "name",
                                "type": "object"
                            }
                        }
                    },
                    "404": {
                        "description": "entreprise not found"
                    }
                }
            }
        }
    }
}